<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="calendarMapper">

	<select id="reserveList" parameterType="String" resultType="Reserve">
		SELECT * FROM RESERVE WHERE MEMBERID=#{memberId}
	</select>
	
	<select id="reserveToday" parameterType="HashMap" resultType="Reserve">
		SELECT * FROM RESERVE WHERE MEMBERID=#{memberId} AND RDATE=#{today}
		ORDER BY RTIME
	</select>
</mapper>
